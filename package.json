{
  "name": "vtsserver",
  "version": "0.1.0",
  "description": "Helmes VTS Server",
  "scripts": {
    "start": "node server/server.js",
    "start-dev": "export NODE_ENV=development || SET \"NODE_ENV=development\" && node server/server.js",
    "start-watch": "export NODE_ENV=development || SET \"NODE_ENV=development\" && nodemon server/server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
    "test-watch": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nodemon --exec \"npm test\""
  },
  "engines": {
    "node": "9.2.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "eslint": "^4.14.0",
    "express": "4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.0-rc1",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "expect": "^21.2.1",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.7",
    "supertest": "^3.0.0"
  }
}
